class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int cnt = 1e9;
        int ans = 0, uttor = 0;
        int n = prices.size();

        for(int i=0; i<n; i++){
            if(prices[i] < cnt){
                cnt = prices[i];
            }
            uttor = prices[i] - cnt;
            if(ans < uttor){
                ans = uttor;
            }
        }
        return ans;
    }
};
